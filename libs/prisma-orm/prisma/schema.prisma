generator client {
  provider = "prisma-client-js"
}

// generator docs {
//   provider = "node ../../node_modules/prisma-docs-generator"
// }

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Seller {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String
}
